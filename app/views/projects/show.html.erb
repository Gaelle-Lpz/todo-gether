<%= @project.name %>

<h2>Members :</h2>
<ul>
  <% @users.each do |user| %>
    <li><%= link_to user.name, user_profile_path(user) %></li>
  <% end %>
</ul>

<h2>tasks : </h2>
<ul>
  <% @tasks.each do |task| %>
    <li><%= link_to task.title, task_path(task) %></li>
  <% end %>
</ul>

<%= link_to "Create Task", new_project_task_path(@project) %>
